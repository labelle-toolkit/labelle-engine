// ImGui GUI with Sokol graphics backend
// Uses dcimgui + sokol_imgui for Dear ImGui integration
.{
    .version = 1,
    .name = "example_gui_sokol",
    .description = "GUI demo with ImGui and Sokol backend",
    .initial_scene = "main",
    .targets = .{.sokol_desktop},
    .gui_backend = .imgui,
    .window = .{
        .width = 800,
        .height = 600,
        .title = "GUI Example (ImGui + Sokol)",
        .target_fps = 60,
    },
    .resources = .{},
    .plugins = .{},
}
