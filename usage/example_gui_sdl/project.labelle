// Nuklear GUI with SDL2 graphics backend
// Uses SDL_Renderer for drawing primitives
.{
    .version = 1,
    .name = "example_gui_sdl",
    .description = "GUI demo with Nuklear and SDL2 backend",
    .initial_scene = "main",
    .backend = .sdl,
    .gui_backend = .nuklear,
    .window = .{
        .width = 800,
        .height = 600,
        .title = "GUI Example (Nuklear + SDL)",
        .target_fps = 60,
    },
    .resources = .{},
    .plugins = .{},
}
